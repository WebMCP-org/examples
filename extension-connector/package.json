{
  "name": "mcp-connector-extension",
  "version": "1.0.0",
  "description": "Example Chrome extension demonstrating how to connect to the MCP-B extension and interact with MCP tools",
  "private": true,
  "type": "module",
  "author": "MCP-B Team",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "mcp",
    "model-context-protocol",
    "mcp-b",
    "browser-automation",
    "ai-tools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/mcp-b",
    "directory": "examples/extension-connector"
  },
  "scripts": {
    "dev": "wxt",
    "dev:chrome": "wxt --browser chrome",
    "build": "wxt build",
    "build:chrome": "wxt build --browser chrome",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --apply .",
    "format": "biome format --write .",
    "clean": "rm -rf .output .wxt node_modules"
  },
  "dependencies": {
    "@mcp-b/transports": "*",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.287",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "wxt": "^0.19.21"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  }
}
